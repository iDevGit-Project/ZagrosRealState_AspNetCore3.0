<!DOCTYPE html>
<html lang="fa">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="سامانه اسناد املاک زاگرس" name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="/images/favicon.ico">

    <!-- jvectormap -->
    <link href="/libs/jqvmap/jqvmap.min.css" rel="stylesheet" />

    <!-- Chartist Chart CSS -->
    <link rel="stylesheet" href="/libs/chartist/chartist.min.css">

    <!-- DataTables -->
    <link href="/libs/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <link href="/libs/datatables/responsive.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <link href="/libs/datatables/buttons.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <script src="~/css/bootstrap.min.js"></script>
    <link href="/libs/bootstrap-select/bootstrap-select.min.css" rel="stylesheet" />

    <!-- Plugins css -->
    <link href="/libs/dropzone/dropzone.min.css" rel="stylesheet" type="text/css" />

    <!-- Custom box css -->
    <link href="/libs/custombox/custombox.min.css" rel="stylesheet">

    <!-- Persian - DateTime Picker -->
    <!-- App css -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/css/app.min.css" rel="stylesheet" type="text/css" />

    <!-- Noty Animated -->
    <link rel="stylesheet" href="/signin/css/animate/animate.min.css">

    <link rel="icon" type="image/png" href="~/images/logo-zagros.png" />

</head>

<body>

    <!-- Begin page -->
    <div id="wrapper">

        <!-- Topbar Start -->
        <div class="navbar-custom">
            <ul class="list-unstyled topnav-menu float-right mb-0">
                @await Html.PartialAsync("_AdminTopBar")
            </ul>

            <ul class="list-unstyled menu-left mb-0">
                <li class="float-left">
                    <a href="#" class="logo">
                        <span class="logo-lg">
                            <img src="/images/logo-light-white.png" alt="" height="50">
                        </span>
                        <span class="logo-sm">
                            <img src="/images/logo-sm.png" alt="" height="50">
                        </span>
                    </a>
                </li>
                @*<li class="float-left">
                    <a class="button-menu-mobile navbar-toggle">
                        <div class="lines">
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </a>
                </li>*@
            </ul>
        </div>
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left-side-menu">

            @await Html.PartialAsync("_AdminLeftMenu")
            <!-- Sidebar -left -->

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <div class="content">

                <!-- START - Content-->
                @RenderBody()
                <!-- END - container -->

            </div> <!-- content -->
            <!-- Footer Start -->
            <footer class="footer">
                @await Html.PartialAsync("_AdminFooter")
            </footer>
            <!-- end Footer -->

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->
    <!-- Right Sidebar -->
    <div class="right-bar">
        @await Html.PartialAsync("_AdminRightMenu")
    </div>
    <!-- /Right-bar -->
    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>

    <!-- Vendor js -->
    <script src="/js/vendor.min.js"></script>

    <!-- KNOB JS -->
    <script src="/libs/jquery-knob/jquery.knob.min.js"></script>
    <!-- Chart JS -->
    <script src="/libs/chart-js/Chart.bundle.min.js"></script>

    <!--Chartist Chart-->
    <script src="/libs/chartist/chartist.min.js"></script>
    <script src="/libs/chartist/chartist-plugin-tooltip.min.js"></script>

    <!-- Init js -->
    <script src="/js/pages/chartist.init.js"></script>

    <!-- Jvector map -->
    <script src="/libs/jqvmap/jquery.vmap.min.js"></script>
    <script src="/libs/jqvmap/jquery.vmap.usa.js"></script>

    <!-- Datatable js -->
    <script src="/libs/datatables/jquery.dataTables.min.js"></script>
    <script src="/libs/datatables/dataTables.bootstrap4.min.js"></script>
    <script src="/libs/datatables/dataTables.responsive.min.js"></script>
    <script src="/libs/datatables/responsive.bootstrap4.min.js"></script>
    <script src="/libs/datatables/buttons.bootstrap4.min.js"></script>
    <script src="/libs/datatables/buttons.html5.min.js"></script>
    <script src="/libs/datatables/buttons.flash.min.js"></script>
    <script src="/libs/datatables/buttons.print.min.js"></script>
    <script src="/libs/datatables/dataTables.keyTable.min.js"></script>
    <script src="/libs/datatables/dataTables.select.min.js"></script>

    <!-- Dashboard Init JS -->
    <script src="/js/pages/dashboard.init.js"></script>

    <!-- App js -->
    <script src="/js/app.min.js"></script>

    <!-- Plugins js -->
    <script src="/libs/dropzone/dropzone.min.js"></script>

    <!-- Modal-Effect -->
    <script src="/libs/custombox/custombox.min.js"></script>

    <!-- Bootstrap Select -->
    <script src="/libs/bootstrap-select/bootstrap-select.min.js"></script>

    <script src="/signin/js/components/notifications/notifications.js"></script>
    <script src="/signin/vendors/js/noty/noty.min.js"></script>

    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.2.0.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.16.0/jquery.validate.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>


    @RenderSection("Scripts", false)
</body>
</html>
